{
    "name": "betobetok/blade-icons",
    "description": "This is a personal extention from https://github.com/blade-ui-kit/blade-icons",
    "keywords": ["Blade", "Icons", "Laravel", "SVG"],
    "homepage": "https://github.com/betobetok/blade-icons-svg",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/betobetok/blade-icons-svg/issues",
        "source": "https://github.com/betobetok/blade-icons-svg"
    },
    "authors": [{
        "name": "Alberto Solorzano Kraemer",
        "email": "betobetok@gmail.com"
    }],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/filesystem": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/view": "^8.0",
        "symfony/console": "^5.3",
        "symfony/finder": "^5.3",
        "numphp/numphp": "^1.2.0 "
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "bin": [
        "bin/blade-icons-generate"
    ],
    "autoload": {
        "psr-4": {
            "BladeUI\\Icons\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BladeUI\\Icons\\BladeIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}