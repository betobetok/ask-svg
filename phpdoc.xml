<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor>
    <parser>
        <default-package-name>ASK-Svg</default-package-name>
        <target>build/api-cache</target>
    </parser>
    <transformer>
        <target>build/documentation</target>
    </transformer>
    <files>
        <directory>.</directory>
        <ignore>build/**/*</ignore>
        <ignore>vendor/**/*</ignore>
    </files>
    <transformations>
        <template name="templates/clean" />
    </transformations>
</phpdocumentor>